#include<bits/stdc++.h>
using namespace std;
int gcd(int a,int b)
{
    if(b==0)
    {
        return a;
    }
    if(a==0)
    {
        return b;
    }
    if(a==b)
    {
        return a;
    }
    return gcd(b,a%b);
}
int lcm(int a,int b)
{
    return ((a*b)/gcd(a,b));
}
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    int a,b;
    cin>>a >>b;
    gcd(a,b);
    cout<<lcm(a,b)<<endl;

}
